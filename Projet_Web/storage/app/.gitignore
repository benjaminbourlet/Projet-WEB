# Composer dependencies
/vendor/

# Node modules
/node_modules/

# Laravel Mix generated files
/public/hot
/public/storage
/public/js
/public/css
/public/mix-manifest.json

# IDE files
/.vscode/
/.idea/
/nbproject/

# MacOS files
.DS_Store

# Thumbnails
Thumbs.db

# Other system files
*.swp
*~
